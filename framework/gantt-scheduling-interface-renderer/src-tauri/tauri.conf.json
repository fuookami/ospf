{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "gantt-scheduling-interface-renderer",
  "version": "0.1.0",
  "identifier": "fuookami.ospf.gantt-scheduling.interface.renderer",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:14000",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "gantt-scheduling-interface-renderer",
        "width": 1600,
        "height": 900,
        "minWidth": 1280,
        "minHeight": 720
      }
    ],
    "security": {
      "csp": null
    },
    "withGlobalTauri": true
  },
  "bundle": {
    "active": true,
    "icon": [
      "icons/icon.ico"
    ],
    "targets": "all",
    "windows": {
      "wix": {
        "language": ["zh-CN"]
      }
    }
  }
}
