{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:12001",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "aps-interface-gantt-scheduling-renderer",
    "version": "0.0.5"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "window": {
        "all": false,
        "close": true,
        "create": true,
        "setSize": true,
        "setTitle": true
      },
      "shell": {
        "all": false,
        "open": true
      },
      "dialog": {
        "all": false,
        "open": true,
        "save": true
      },
      "path": {
        "all": true
      }
    },
    "bundle": {
      "active": true,
      "icon": [
        "icons/icon.ico"
      ],
      "identifier": "com.tauri.build",
      "targets": "all",
      "windows": {
        "wix": {
          "language": ["zh-CN"]
        }
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "label": "main",
        "fullscreen": false,
        "resizable": true,
        "title": "aps-interface-gantt-scheduling-renderer",
        "width": 1600,
        "height": 900,
        "minWidth": 1280,
        "minHeight": 720
      }
    ]
  }
}
